EXE 3 -
{ 
	int x0 = LerNumeroInteiro();
            int x1 = LerNumeroInteiro();
            int x2 = LerNumeroInteiro();

            if (x0 > (x1 + x2))
            { Console.WriteLine("O primeiro numero digitado e maior que a soma dos outros digitados");
            }
            if (x0 < (x1 + x2))
            { Console.WriteLine("O primeiro numero digitado nao e maior que a soma dos outros digitados");
            }
            if (x0 == (x1 + x2))
            { Console.WriteLine("O primeiro numero digitado e igual que a soma dos outros digitados");
            }
        }

        static int LerNumeroInteiro()
        {
            Console.WriteLine("Digite um numero");
            int numero = Convert.ToInt32(Console.ReadLine());
            return numero;
}
