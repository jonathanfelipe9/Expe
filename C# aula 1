Exercicio 1
{

        int x;
        int y;

        
            Console.WriteLine("Digite um numero");
            x = Convert.ToInt32(Console.ReadLine());

            do
            {
                Console.WriteLine("Digite um numero");
                y = Convert.ToInt32(Console.ReadLine());

            }
            while (y < x);

            for (int i = x+1 ; i < y; i++)
            {
                Console.WriteLine(i);
                    
                  
            }


            Console.ReadLine();

            
        
        }
